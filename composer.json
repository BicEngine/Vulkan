{
    "name": "bic-engine/vulkan",
    "license": "MIT",
    "type": "library",
    "description": "PHP Vulkan bindings",
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "bic-engine/preprocessor": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bic\\Vulkan\\": "src"
        }
    },
    "require-dev": {
        "fzaninotto/faker": "~1.9",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^5.0",
        "vimeo/psalm": "^4.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\Vulkan\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "@php ./vendor/vimeo/psalm/psalm"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
